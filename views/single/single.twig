  {# allows .uk-navbar-toggle-animate to work on uk-toggles with #offcanvas-overlay as target. needs anim-target attribute #}
  <script>
    var animated_toggles = document.querySelectorAll('.uk-navbar-toggle-animate');
    animated_toggles.forEach(item => {

      if(item.getAttribute('anim-target')){

        var theTarget = document.querySelector(item.getAttribute('anim-target'))

        theTarget.addEventListener("show", (event) => {
          if (event.target.id === 'offcanvas-overlay') {
            if(item.ariaExpanded == 'false') item.ariaExpanded = 'true';
          }
        });

        theTarget.addEventListener("hide", (event) => {
          if (event.target.id === 'offcanvas-overlay') {
            if(item.ariaExpanded == 'true') item.ariaExpanded = 'false';
          }
        });

      }

    });
  </script>

