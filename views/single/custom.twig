{% extends 'single.twig' %}

{% block header %}
  {# {{ parent() }} #}
{% endblock %}

{% block styles %}
  {{ parent() }}
  {# google_fonts #}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Mulish:wght@300;400;500;600;700;800;900&display=swap" rel="stylesheet">
  {# /google_fonts #}
  <style>

    {# vars #}
      {% set green_colour = '#608853' %}
      {% set green_colour_seethru = '#608853e6' %}
      {% set black_colour = '#000000' %}
      {% set black_colour_seethru = '#000000cc' %}
      {% set white_colour = '#ffffff' %}
      {% set white_colour_seethru = '#ffffffcc' %}
      {% set yellow_colour = '#e4e400' %}
    {# /vars #}

    {# text #}

      html {
        background: #fff;
        color: #666;
        font-family: -apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Helvetica Neue,Arial,Noto Sans,sans-serif,Apple Color Emoji,Segoe UI Emoji,Segoe UI Symbol,Noto Color Emoji;
        font-size: 16px;
        font-weight: 500;
        line-height: 1.5;
        font-family: 'Mulish', sans-serif;
      }
      .uk-h1, .uk-h2, .uk-h3, .uk-h4, .uk-h5, .uk-h6, .uk-heading-2xlarge, .uk-heading-large, .uk-heading-medium, .uk-heading-small, .uk-heading-xlarge, h1, h2, h3, h4, h5, h6 {
        color: #333;
        font-family: -apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Helvetica Neue,Arial,Noto Sans,sans-serif,Apple Color Emoji,Segoe UI Emoji,Segoe UI Symbol,Noto Color Emoji;
        font-weight: 900;
        font-family: 'Mulish', sans-serif;
      }

      .uk-light .uk-text-emphasis {
        color: #ffffff;
      }

      {# primary: red #}
      .uk-text-primary, .uk-light .uk-text-primary, .uk-section-primary.uk-light .uk-text-primary {
        color: {{green_colour}}!important;
      }
      {# secondary: black #}
      .uk-text-emphasis, .uk-light .uk-text-secondary {
        color: {{black_colour}}!important;
      }
      {# youtube popup icon #}
      .uk-light .uk-icon-link {
        color: hsla(0,0%,100%,1);
        transition: .1s ease-in-out;
        transition-property: color;
      }
      .uk-light .uk-icon-link:hover {
        color: {{green_colour}};
      }

      {# BIG headings #}
      .uk-heading-medium {
        font-size: 2.8875rem;
        line-height: 1.1;
      }
      @media (min-width: 960px){
        .uk-heading-medium {
          font-size: 3.5rem;
        }
      }
      @media (min-width: 1200px){
        .uk-heading-medium {
          font-size: 4.5rem;
        }
      }
      .uk-heading-large {
        font-size: 5rem;
      }
      @media (min-width: 960px){
        .uk-heading-large {
          font-size: 7rem;
        }
      }
      @media (min-width: 1200px){
        .uk-heading-large {
          font-size: 9rem;
        }
      }

      {# text effect/s #}
      .bigger-lines {
        line-height: 1.9rem;
      }

      {# leads #}
      .uk-text-lead {
        font-size: 1.2rem;
        line-height: 1.2;
      }
      @media (min-width: 1200px){
        .uk-text-lead {
          font-size: 1.5rem;
          line-height: 1.5;
        }
      }

    {# /text #}

    {# backgrounds #}

      {# primary: red #}
      .uk-background-primary, .uk-section-primary {
        background-color: {{green_colour}};
      }
      .overlay-primary:after {
        background-color: {{green_colour_seethru}};
      }
      .uk-overlay-primary {
        background: {{green_colour_seethru}};
      }
      {# secondary: black #}
      .uk-section-secondary, .uk-background-secondary, .black-bg {
        background-color: {{black_colour}};
      }
      .overlay-secondary:after {
        background-color: {{black_colour_seethru}};
      }
      .uk-overlay-secondary {
        background: {{black_colour_seethru}};
      }
      .overlay-default:after {
        background-color: {{white_colour_seethru}};
      }

    {# /backgrounds #}

    {# geometry #}

      {# float box (pull up image) height #}
      .float-box.uk-height-small {
        height: 80px;
      }
      @media (min-width: 1200px){
        .float-box.uk-height-small {
          height: 200px;
        }
      }

      {# product container grid #}
      .uk-grid-large.uk-grid-column-large {
          margin-left: -70px;
      }
      @media (min-width: 1200px){
        .uk-grid-large.uk-grid-column-large {
          margin-left: -100px;
        }
      }
      .uk-grid-large.uk-grid-column-large>* {
        padding-left: 70px;
      }
      @media (min-width: 1200px){
        .uk-grid-large.uk-grid-column-large>* {
          padding-left: 100px;
        }
      }

    {# /geometry #}

    {# buttons #}

      .uk-button {
        text-transform: capitalize;
        font-weight: 700;
      }
      .uk-button-large {
        border-radius: 100px;
        font-size: 1rem;
        line-height: 80px;
        padding: 0 80px;
      }
      .uk-button-primary,
      .uk-cover-container.uk-light .uk-button-primary,
      .uk-icon-button.uk-background-primary {
        background-color: {{green_colour}}!important;
        color: {{white_colour}};
      }
      .uk-button-secondary {
        background-color: {{black_colour}};
      }
      .uk-light .uk-button-secondary {
        background-color: {{black_colour}};
        color: {{white_colour}};
      }

    {# /buttons #}

    {# borders #}
      .border-rounded-full {
        border-radius: 100%;
      }
    {# /borders #}

    {# transitions  & effects #}
      [class*=uk-transition-spin] {
        transition: .3s ease-out;
        transition-property: transform;
      }
      .uk-transition-active.uk-active .uk-transition-spin-round, .uk-transition-toggle .uk-transition-spin-round:focus-within, .uk-transition-toggle:focus .uk-transition-spin-round, .uk-transition-toggle:hover .uk-transition-spin-round {
        transform: rotate(30deg);
      }
      .uk-transition-active.uk-active .scale-up-bigger, .uk-transition-toggle .scale-up-bigger:focus-within, .uk-transition-toggle:focus .scale-up-bigger, .uk-transition-toggle:hover .scale-up-bigger {
        --uk-scale-x: 1.10;
        --uk-scale-y: 1.10;
      }
      .bg-scale-up {
        background-size: auto 100%;
        background-repeat: no-repeat;
        background-position: center;
        transition: background-size .3s ease-out;
      }
      .bg-scale-up:hover {
        background-size: auto 110%!important;
      }
      .uk-navbar-toggle-animate svg[class*="line-"] {
        transition:0.2s ease-in-out;
        transition-property:transform, opacity;
        transform-origin:center;
        opacity:1
      }
      .uk-navbar-toggle svg.line-3 {
        opacity:0
      }
      .uk-navbar-toggle-animate[aria-expanded="true"] svg.line-3 {
        opacity:1
      }
      .uk-navbar-toggle-animate[aria-expanded="true"] svg.line-2 {
        transform:rotate(45deg)
      }
      .uk-navbar-toggle-animate[aria-expanded="true"] svg.line-3 {
        transform:rotate(-45deg)
      }
      .uk-navbar-toggle-animate[aria-expanded="true"] svg.line-1,
      .uk-navbar-toggle-animate[aria-expanded="true"] svg.line-4 {
        opacity:0
      }
      .uk-navbar-toggle-animate[aria-expanded="true"] svg.line-1 {
        transform:translateY(6px) scaleX(0)
      }
      .uk-navbar-toggle-animate[aria-expanded="true"] svg.line-4 {
        transform:translateY(-6px) scaleX(0)
      }
      .uk-navbar-toggle-animate svg {
        width: 32px;
        height: 32px;
      }
      .lte-effect-smoke {
        position: relative;
        pointer-events: none;
      }
      .lte-effect-smoke img {
        zoom: 1;
        filter: alpha(opacity=0);
        -webkit-opacity: 0;
        -moz-opacity: 0;
        opacity: 0;
        position: absolute;
        bottom: 0%;
        left: 50%;
        margin: 0 0 300px;
        -webkit-transform: translate(-50%, 0px);
        -moz-transform: translate(-50%, 0px);
        -ms-transform: translate(-50%, 0px);
        -o-transform: translate(-50%, 0px);
        transform: translate(-50%, 0px);
        -webkit-animation: lte-smoke-1 12s infinite;
        -moz-animation: lte-smoke-1 12s infinite;
        -o-animation: lte-smoke-1 12s infinite;
        animation: lte-smoke-1 12s infinite;
      }
      .lte-effect-smoke .lte-effect-item-2 {
        animation-delay: 2s;
      }
      .lte-effect-smoke .lte-effect-item-3 {
        animation-delay: 4s;
      }
      .lte-effect-smoke .lte-effect-item-4 {
        animation-delay: 6s;
      }
      .lte-effect-smoke .lte-effect-item-5 {
        animation-delay: 8s;
      }
      .lte-effect-smoke .lte-effect-item-6 {
        animation-delay: 10s;
      }
      .lte-effect-smoke .lte-effect-item-2 img {
        animation-delay: 2s;
        -webkit-transform: scaleX(-1);
        -moz-transform: scaleX(-1);
        -ms-transform: scaleX(-1);
        -o-transform: scaleX(-1);
        transform: scaleX(-1);
      }
      .lte-effect-smoke .lte-effect-item-4 img {
        animation-delay: 6s;
        -webkit-transform: scaleX(-1);
        -moz-transform: scaleX(-1);
        -ms-transform: scaleX(-1);
        -o-transform: scaleX(-1);
        transform: scaleX(-1);
      }
      .lte-effect-smoke .lte-effect-item-6 img {
        animation-delay: 10s;
        -webkit-transform: scaleX(-1);
        -moz-transform: scaleX(-1);
        -ms-transform: scaleX(-1);
        -o-transform: scaleX(-1);
        transform: scaleX(-1);
      }
      @keyframes lte-smoke-1 {
        0% {
          opacity: 0;
          transform: translate(-50%, 650px);
        }
        50% {
          opacity: .5;
        }
        100% {
          opacity: 0;
          transform: translateY(-50%, 0px);
        }
      }
    {# /transitions #}

    {# number badges: custom element #}
      .number-badge-wrap {
        left: -40px;
        top: calc(50% - 40px);
      }
      .number-badge {
        width: 100px;
        height: 100px;
        font-weight: 700;
        font-size: 1.5rem;
        color: #fff;
        border-radius: 100%;
        position: absolute;
        opacity: 1;
      }
    {# /number #}

    {# cover & pull-up wrap #}
      .coffee_bean-container {
        max-width: 500px;
      }
      @media (min-width: 1200px){
        .coffee_bean-container {
          max-width: none;
        }
      }
    {# /cover #}

    {# nav/bar: main #}
      .main-nav .uk-navbar-item, .main-nav .uk-navbar-nav>li>a, .main-nav .uk-navbar-toggle {
        font-size: 1.2rem;
        padding: 0px 15px;
        text-transform: capitalize;
        min-height: 140px;
      }
      @media (min-width: 1200px){
        .main-nav .uk-navbar-item, .main-nav .uk-navbar-nav>li>a, .main-nav .uk-navbar-toggle {
          min-height: 200px;
        }
      }
      .uk-light .uk-navbar-nav>li>a, .uk-light .uk-navbar-toggle  {
        color: hsla(0,0%,100%,1);
      }
      .uk-light .uk-navbar-nav>li.uk-active>a {
        color: {{green_colour}};
      }
      .uk-icon-button.uk-icon-button-large {
        height: 50px;
        width: 50px;
      }
      .uk-light .uk-icon-button {
        background-color: hsla(0,0%,100%,.1);
        color: hsla(0,0%,100%,1);
      }
      .uk-light .uk-icon-button:hover {
        background-color: #3e845b;
        color: hsla(0,0%,100%,1);
      }
    {# /nav #}

    {# util #}
      .large-right {
        padding-right: 100px;
      }
      .full-height-viewport {
        height: 100vh;
      }
    {# /util #}

    {# lists #}
      .uk-list-xlarge>*>ul, .uk-list-xlarge>:nth-child(n+2) {
        margin-top: 50px;
      }
    {# /lists #}

    {# dividers #}
      .uk-divider-small:after {
        border-top: 8px solid #608853;
        border-radius: 100px;
        width: 60px;
      }
      .primary-divider:after {
        width: 250px;
        border-radius: 0;
        border-color: #608853!important;
        border-width: medium;
      }
    {# /dividers #}

    {# Swiper styles #}
      .swiper, swiper-container {
        padding: 0;
      }
      @media (min-width: 640px){
        .swiper, swiper-container {
          padding: 0 20px;
        }
      }
      .uk-light .swiper-pagination-bullet {
        background: #fff;
      }
      .swiper-pagination-bullet-active {
        background: #608853!important;
      }
    {# /Swiper styles #}

    {# footer #}
      footer .uk-logo .uk-width-medium {
        width: 250px;
      }
      .footer-nav {
        column-count: 2;
        column-gap: 10px;
        font-size: 1.2rem;
      }
      .footer-nav>* {
        padding-left: 20px;
      }
    {# /footer #}

    .uk-offcanvas-bar {
      width: 86%;
      max-width: 450px!important;
    }

    .uk-width-xsmall {
      width: 100px;
    }

    .uk-padding-medium {
      padding: 20px;
    }

    .uk-nav-default>li>a {
      color: #333;
    }
    .uk-nav-default>li.uk-active>a {
      color: {{green_colour}};
    }
    .uk-nav>li>a {
      font-weight: 800;
    }

    .uk-offcanvas-bar .uk-nav-default {
      font-size: 1.1rem;
    }
    .uk-offcanvas-bar .uk-nav-default li>a {
      display: block;
    }
    .uk-offcanvas-bar .uk-nav-default li.uk-parent>a::before {
      content: "";
      width: 1.4em;
      height: 1.4em;
      margin-left: 10px;
      float: right;
      background-image: url(data:image/svg+xml;charset=UTF-8,%3Csvg%20width%3D%2213%22%20height%3D%2213%22%20viewBox%3D%220%200%2013%2013%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%0A%20%20%20%20%3Crect%20fill%3D%22%23666%22%20width%3D%2213%22%20height%3D%222%22%20x%3D%220%22%20y%3D%226%22%20%2F%3E%0A%20%20%20%20%3Crect%20fill%3D%22%23666%22%20width%3D%222%22%20height%3D%2213%22%20x%3D%226%22%20y%3D%220%22%20%2F%3E%0A%3C%2Fsvg%3E);
      background-repeat: no-repeat;
      background-position: 50% 50%;
    }
    .uk-offcanvas-bar .uk-nav-default li.uk-parent.uk-open>a::before {
      background-image: url(data:image/svg+xml;charset=UTF-8,%3Csvg%20width%3D%2213%22%20height%3D%2213%22%20viewBox%3D%220%200%2013%2013%22%20xmlns%3D%22http%3A%2F%2Fwww.w3.org%2F2000%2Fsvg%22%3E%0A%20%20%20%20%3Crect%20fill%3D%22%23666%22%20width%3D%2213%22%20height%3D%222%22%20x%3D%220%22%20y%3D%226%22%20%2F%3E%0A%3C%2Fsvg%3E);
    }

  </style>
{% endblock %}

{% block main %}

  <header class="uk-position-top uk-position-z-index uk-light">
    <nav class="main-nav uk-navbar-container uk-navbar-transparent">
      <div class="uk-container uk-container-expand">
        <div uk-navbar>

          {# main_nav_left_socials #}
          {% if has_menu_socials %}
            <div class="uk-navbar-left uk-visible@m">
              <div class="uk-navbar-item uk-flex uk-flex-left uk-flex middle uk-padding-remove-left">
                {% for item in menu_socials.get_items %}
                  <a href="{{ item.get_link|e('esc_url') }}" class="uk-icon-button uk-icon-button-large uk-margin-small-right" uk-icon="icon: {{ item.title|lower|e('esc_html')}}; ratio: 1.5"></a>
                {% endfor %}
              </div>
            </div>
          {% endif %}

          {# main_nav_center_main_menu #}
          <div class="uk-navbar-center uk-flex uk-flex-middle">

            {# main_nav_center_main_menu_left #}
            {% if has_menu_main %}
              {% if menu_main.first_half_items is not empty %}
                <div class="uk-flex-left uk-visible@m">
                  <ul class="uk-navbar-nav">
                    {% for item in menu_main.first_half_items %}
                      {% if item.get_children %}
                        <li class="{{ item.classes|join(' ')|e('esc_html') }}">
                          <a href="{{ item.link|e('esc_url') }}">{{ item.title|e('esc_html') }}</a>
                          <div uk-dropdown>
                            <ul class="uk-nav uk-dropdown-nav">
                              {% for child in item.get_children %}
                                <li class="{{ child.classes|join(' ')|e('esc_html') }}">
                                  <a href="{{ child.get_link|e('esc_url') }}">{{ child.title|e('esc_html') }}</a>
                                  {% if child.children %}
                                    <div uk-dropdown="pos: right-top">
                                      <ul class="uk-nav uk-dropdown-nav">
                                        {% for third in child.children %}
                                          <li class="{{ third.classes|join(' ')|e('esc_html') }}">
                                            <a href="{{ third.get_link|e('esc_url') }}">{{ third.title|e('esc_html') }}</a>
                                          </li>
                                        {% endfor %}
                                      </ul>
                                    </div>
                                  {% endif %}
                                </li>
                              {% endfor %}
                            </ul>
                          </div>
                        </li>
                      {% else %}
                        <li class="{{ item.classes|join(' ')|e('esc_html') }}">
                          <a href="{{ item.link|e('esc_url') }}">{{ item.title|e('esc_html') }}</a>
                        </li>
                      {% endif %}
                    {% endfor %}
                  </ul>
                </div>
              {% endif %}
            {% endif %}

            {# main_nav_center_logo_center #}
            <a class="uk-navbar-item uk-logo" href="#">
              {% if theme.logo %}
                {# <img src="{{ theme.link }}/assets/img/coffee/logo/3D_white_green.png" alt="" class="uk-width-small uk-width-medium@m" hidden> #}
                <img src="{{ theme.logo.src|e('esc_url') }}" width="{{ theme.logo.w }}" height="{{ theme.logo.h }}" alt="{{ theme.logo.alt|e('esc_html') }}">
              {% else %}
                {# {{ site.title|e('esc_html') }} #}
                <img src="{{ theme.link }}/assets/img/coffee/logo/3D_white_green.png" alt="" class="uk-width-small uk-width-medium@m">
              {% endif %}
            </a>

            {# main_nav_center_main_menu_right #}
            {% if has_menu_main %}
              {% if menu_main.second_half_items is not empty %}
                <div class="uk-flex-right uk-visible@m">
                  <ul class="uk-navbar-nav">
                    {% for item in menu_main.second_half_items %}
                      {% if item.get_children %}
                        <li class="{{ item.classes|join(' ')|e('esc_html') }}">
                          <a href="{{ item.link|e('esc_url') }}">{{ item.title|e('esc_html') }}</a>
                          <div uk-dropdown>
                            <ul class="uk-nav uk-dropdown-nav">
                              {% for child in item.get_children %}
                                <li class="{{ child.classes|join(' ')|e('esc_html') }}">
                                  <a href="{{ child.get_link|e('esc_url') }}">{{ child.title|e('esc_html') }}</a>
                                  {% if child.children %}
                                    <div uk-dropdown="pos: right-top">
                                      <ul class="uk-nav uk-dropdown-nav">
                                        {% for third in child.children %}
                                          <li class="{{ third.classes|join(' ')|e('esc_html') }}">
                                            <a href="{{ third.get_link|e('esc_url') }}">{{ third.title|e('esc_html') }}</a>
                                          </li>
                                        {% endfor %}
                                      </ul>
                                    </div>
                                  {% endif %}
                                </li>
                              {% endfor %}
                            </ul>
                          </div>
                        </li>
                      {% else %}
                        <li class="{{ item.classes|join(' ')|e('esc_html') }}">
                          <a href="{{ item.link|e('esc_url') }}">{{ item.title|e('esc_html') }}</a>
                        </li>
                      {% endif %}
                    {% endfor %}
                  </ul>
                </div>
              {% endif %}
            {% endif %}

          </div>

          {# main_nav_right_mobile_toggle #}
          {% if has_menu_main %}
            <div class="uk-navbar-right">
              <button class="uk-navbar-toggle uk-navbar-toggle-animate uk-padding-remove-right" uk-navbar-toggle-icon aria-expanded="false" type="button" uk-toggle="target: #offcanvas-overlay" anim-target="#offcanvas-overlay"></button>
            </div>
          {% endif %}

        </div>
      </div>
    </nav>
  </header>

  {% if has_menu_main %}
    <div id="offcanvas-overlay" class="" uk-offcanvas="overlay: true">
      <div class="uk-offcanvas-bar uk-padding-remove">
        <div class="overlay-wrap overlay-default uk-dark uk-width-1-1 uk-height-1-1 uk-padding-medium" style="background-image: url(https://picsum.photos/id/20/1920/800);">

          <a class="uk-offcanvas-close uk-position-small uk-text-emphasis"><span uk-icon="icon: close; ratio: 1.5"></span></a>

          <div class=" uk-position-relative uk-position-z-index">
            <a class="uk-logo" href="/">
              {# <img src="{{ theme.link }}/assets/img/coffee/demo/light_logo.png" alt="" class="uk-width-xsmall"> #}
            </a>

            {# mobile menu nav #}
            <ul class="uk-nav-default uk-margin" uk-nav="multiple: true">
              {% if has_menu_main %}

                {# mobile menu title #}
                <li class="uk-nav-header uk-padding-remove uk-margin-small-bottom uk-text-capitalize" hidden>
                  {{ menu_main.title|e('esc_html') }}
                  <a class="uk-offcanvas-close uk-position-top-right uk-position-small uk-padding-remove"><span uk-icon="icon: close; ratio: .9"></span></a>
                </li>

                <li class="uk-nav-divider" hidden></li>

                {# mobile menu items #}
                {% for item in menu_main.get_items %}
                  {% if item.get_children %}
                    <li class="{{ item.classes|join(' ')|e('esc_html') }} uk-parent">
                      <a href="{{ item.link|e('esc_url') }}">{{ item.title|e('esc_html')}} <span class="uk-icon uk-nav-parent-icon" uk-icon="icon: check; ratio: 2" hidden></span></a>
                      <ul class="uk-nav-sub">
                        {% for child in item.get_children %}
                          <li class="{{ child.classes|join(' ')|e('esc_html') }}">
                            <a href="{{ child.get_link|e('esc_url') }}">- {{ child.title|e('esc_html')}}</a>
                            {% if child.children %}
                              <ul class="uk-nav-sub">
                                {% for third in child.children %}
                                  <li class="{{ third.classes|join(' ')|e('esc_html') }}">
                                    <a href="{{ third.get_link|e('esc_url') }}">- {{ third.title|e('esc_html')}}</a>
                                  </li>
