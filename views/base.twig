{% block html_head_container %}

  {% include '_html-head.twig' %}
    {% block styles %}
    {% endblock %}
  </head>
  {% endblock %}

  <body class="{{ body_class }}">
    
    {% block theme_preloader_markup %}
      {% if configs.theme_preloader %}
        <div id="Preloader" class="preloader"></div>
      {% endif %}
    {% endblock %}
    
    {% do action('theme_before_header') %}

    {% block header %}
      {# include 'header.twig' #}
    {% endblock %}
    
    <main id="Main" role="main" style="background-color: #fff;">
      <div id="ContentSection">
        {% block main %}
          Sorry, nothing to display here...
        {% endblock %}
      </div>
    </main>
    
    {% block footer %}
      {# include 'footer.twig' #}
    {% endblock %}
    
    {% do action('theme_above_footer') %}
    
    {{ fn('wp_footer') }}
    
    {% block theme_preloader_scripts %}
      {% if configs.theme_preloader %}
        <script type='text/javascript'>
          document.body.addEventListener("load", window.parent.PreloadMe());
        </script>
      {% endif %}
    {% endblock %}
    
    {% block scripts %}
    {% endblock %}
    
  </body>

</html>